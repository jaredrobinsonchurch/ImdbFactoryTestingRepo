{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureDatabricksDeltaLakeSink",
						"importSettings": {
							"type": "AzureDatabricksDeltaLakeImportCommand"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['nconst']"
								}
							},
							{
								"source": {
									"path": "$['primaryName']"
								}
							},
							{
								"source": {
									"path": "$['birthYear']"
								}
							},
							{
								"source": {
									"path": "$['deathYear']"
								}
							},
							{
								"source": {
									"path": "$['primaryProfession'][0]"
								}
							},
							{
								"source": {
									"path": "$['knownForTitles'][0]"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "namebasics",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureDatabricksDeltaLakeDataset1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}